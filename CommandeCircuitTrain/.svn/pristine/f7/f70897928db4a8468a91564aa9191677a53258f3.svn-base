package upmc.train.model.communication.exception;

public class CommunicationUSBException extends Exception 
{
	/**
	 * 
	 */
	private static final long serialVersionUID = 7891193559679909687L;
	private String message = "" ;
	public CommunicationUSBException(String string) 
	{
		this.message = string ;
	}
	
	public String getMessage()
	{
		return message ; 
	}

}
